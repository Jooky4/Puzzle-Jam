; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pazzle Jam"
run/main_scene="uid://3tj3dir3mrf4"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

YandexSDK="*res://addons/godot-yandex-games-sdk/yandex_sdk.gd"
LevelManager="*res://Scenes/Global/Level_manager.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=1280
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"
window/handheld/orientation=1

[file_customization]

folder_colors={
"res://Resources/": "yellow",
"res://Resources/Fonts/": "pink",
"res://Resources/Music/": "blue",
"res://Resources/Shaders/": "teal",
"res://Resources/Sounds/": "blue",
"res://Scenes/": "red",
"res://Texture/": "green"
}

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true
